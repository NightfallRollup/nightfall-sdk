{
  "name": "nightfall-sdk",
  "version": "1.1.2",
  "description": "SDK for interacting with Nightfall",
  "keywords": [
    "nightfall",
    "sdk",
    "nightfall_3",
    "nf3"
  ],
  "main": "./dist/cjs/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "types": "./dist/types/user/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NightfallRollup/nightfall-sdk.git"
  },
  "license": "CC0-1.0",
  "dependencies": {
    "async-mutex": "^0.4.0",
    "axios": "^0.27.2",
    "bip39": "^3.0.4",
    "dotenv": "^16.0.1",
    "events": "^3.3.0",
    "general-number": "^1.0.1",
    "joi": "^17.6.0",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "queue": "^6.0.2",
    "reconnecting-websocket": "^4.4.0",
    "web3": "^1.7.3",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/events": "^3.0.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.11.18",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-jest": "^28.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "prettier": "2.6.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "rm -rf ./dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS -outDir dist/cjs",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --check .",
    "test:ganache": "DOTENV_CONFIG_PATH=./examples/scripts/.env.ganache jest --setupFiles dotenv/config",
    "test:goerli": "DOTENV_CONFIG_PATH=./examples/scripts/.env.goerli jest --setupFiles dotenv/config",
    "eg:ganache:deposit": "ts-node -r dotenv/config examples/scripts/txDeposit.ts dotenv_config_path=./examples/scripts/.env.ganache dotenv_config_debug=true",
    "eg:ganache:minting": "ts-node -r dotenv/config examples/scripts/l2TokenisationMinting.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:transfer": "ts-node -r dotenv/config examples/scripts/txTransfer.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:burning": "ts-node -r dotenv/config examples/scripts/l2TokenisationBurning.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawal.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:finalise-withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawalFinalise.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:balances": "ts-node -r dotenv/config examples/scripts/balances.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:export-commitments": "ts-node -r dotenv/config examples/scripts/commitmentsExport.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:ganache:import-commitments": "ts-node -r dotenv/config examples/scripts/commitmentsImport.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "utils:ganache:mint-721": "ts-node -r dotenv/config examples/scripts/utils/mint721.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "utils:ganache:contract-addresses": "ts-node -r dotenv/config examples/scripts/utils/ercMockContractAddresses.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "utils:ganache:l2-keys": "ts-node -r dotenv/config examples/scripts/utils/nightfallKeys.ts dotenv_config_path=./examples/scripts/.env.ganache",
    "eg:goerli:deposit": "ts-node -r dotenv/config examples/scripts/txDeposit.ts dotenv_config_path=./examples/scripts/.env.goerli dotenv_config_debug=true",
    "eg:goerli:transfer": "ts-node -r dotenv/config examples/scripts/txTransfer.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawal.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:finalise-withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawalFinalise.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:balances": "ts-node -r dotenv/config examples/scripts/balances.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:export-commitments": "ts-node -r dotenv/config examples/scripts/commitmentsExport.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:goerli:import-commitments": "ts-node -r dotenv/config examples/scripts/commitmentsImport.ts dotenv_config_path=./examples/scripts/.env.goerli",
    "eg:edge:deposit": "ts-node -r dotenv/config examples/scripts/txDeposit.ts dotenv_config_path=./examples/scripts/.env.edge dotenv_config_debug=true",
    "eg:edge:transfer": "ts-node -r dotenv/config examples/scripts/txTransfer.ts dotenv_config_path=./examples/scripts/.env.edge",
    "eg:edge:withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawal.ts dotenv_config_path=./examples/scripts/.env.edge",
    "eg:edge:finalise-withdrawal": "ts-node -r dotenv/config examples/scripts/txWithdrawalFinalise.ts dotenv_config_path=./examples/scripts/.env.edge",
    "eg:edge:balances": "ts-node -r dotenv/config examples/scripts/balances.ts dotenv_config_path=./examples/scripts/.env.edge",
    "eg:edge:export-commitments": "ts-node -r dotenv/config examples/scripts/commitmentsExport.ts dotenv_config_path=./examples/scripts/.env.edge",
    "eg:edge:import-commitments": "ts-node -r dotenv/config examples/scripts/commitmentsImport.ts dotenv_config_path=./examples/scripts/.env.edge",
    "start-react-app": "npm run build && cd examples/web-app/ && npm start"
  }
}
